{
  "name": "CodeCharm",
  "displayName": "CodeCharm",
  "description": "AI-powered code comment generator for VS Code using Gemini",
  "version": "0.2.1",
  "publisher": "DeveloperPuneet",
  "icon": "images/Icon.png",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:CodeCharm.commentFunction"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "CodeCharm.commentFunction",
        "title": "ðŸ§  CodeCharm: Generate Comment (Ctrl+Win+J)"
      }
    ],
    "keybindings": [
      {
        "command": "CodeCharm.commentFunction",
        "key": "ctrl+win+j",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "CodeCharm Settings",
      "properties": {
        "CodeCharm.apiKey": {
          "type": "string",
          "default": "AIzaSyCmn7yKuV9XsIKNfgR07o7fdXyYdbQVuyU",
          "description": ""
        }
      }
    }
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DeveloperPuneet/CodeCharm.git"
  }
}