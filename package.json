{
  "name": "codewhiz",
  "displayName": "CodeWhiz",
  "description": "AI-powered code comment generator for VS Code using Gemini",
  "version": "0.0.1",
  "publisher": "codewhiz",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codewhiz.commentFunction"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codewhiz.commentFunction",
        "title": "ðŸ§  CodeWhiz: Generate Comment (Ctrl+Win+J)"
      }
    ],
    "keybindings": [
      {
        "command": "codewhiz.commentFunction",
        "key": "ctrl+win+j",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "CodeWhiz Settings",
      "properties": {
        "codewhiz.apiKey": {
          "type": "string",
          "default": "AIzaSyCmn7yKuV9XsIKNfgR07o7fdXyYdbQVuyU",
          "description": ""
        }
      }
    }
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
